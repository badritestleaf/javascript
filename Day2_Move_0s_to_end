let arr = [0,4,5,8,0,6];
let temp = 0;
let start = 0;
let end = 0;

while(end < arr.length)
{
    if(arr[end] == 0)
    {
        end++;
    }
    else
    {
        temp = arr[start];
        arr[start] = arr[end];
        arr[end] = temp;
        start++;
        end++;
    }
}

console.log(arr);
