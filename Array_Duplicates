let arr = [6,1,2,3,1,4,6,6];

arr.sort();

let outputArr = [];
let index = 0;
let tempIndex = 1;
let flag = 0;

while(index<arr.length)
{
    tempIndex = 1;
    while(arr[index]==arr[index+1])
    {
        tempIndex++;
    }

    if(tempIndex==1)
    {
        outputArr.push(arr[index]);
    }
    index++;
}

console.log(outputArr);
